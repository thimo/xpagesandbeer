	<!--
		Use this pattern to define property name/value pairs for controls
	-->
	<!--
		<control> <name>[Control Name]</name> <property> <name>[property Name]</name> <value>[property Value]</value>
		</property> </control>
	-->

	<control>
		<name>Button.Command</name>
		<property mode="concat">
			<name>styleClass</name>
			<value>btn btn-default</value>
		</property>
	</control>
	
	<control>
		<name>Button</name>
		<property mode="concat">
			<name>styleClass</name>
			<value>btn btn-default</value>
		</property>
	</control>

	<control>
		<name>Button.Submit</name>
		<property mode="concat">
			<name>styleClass</name>
			<value>btn btn-default</value>
		</property>
		<property>
			<name>type</name>
			<value>submit</value>
		</property>
	</control>

	<control>
		<name>ListBox</name>
		<property mode="concat">
			<name>styleClass</name>
			<value>form-control</value>
		</property>
	</control>

	<control>
		<name>ComboBox</name>
		<property mode="concat">
			<name>styleClass</name>
			<value>form-control</value>
		</property>
	</control>

	<control>
		<name>Link</name>
		<property>
			<name>styleClass</name>
			<value></value>
		</property>
	</control>

	<control>
		<name>Text.ComputedField</name>
		<property>
			<name>styleClass</name>
			<value></value>
		</property>
	</control>

	<control>
		<name>Form</name>
		<property>
			<name>styleClass</name>
			<value></value>
		</property>
	</control>

	<control>
		<name>Text.Label</name>
		<property mode="concat">
			<name>styleClass</name>
			<value>control-label</value>
		</property>
	</control>

	<control>
		<name>InputField.EditBox</name>
		<property>
			<name>styleClass</name>
			<value>form-control</value>
		</property>
	</control>

	<control>
		<name>InputField.Secret</name>
		<property>
