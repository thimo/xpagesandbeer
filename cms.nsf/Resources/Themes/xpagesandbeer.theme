		</property>
	</control>

	<control>
		<name>ViewRoot</name>
		<property>
			<name>pageIcon</name>
			<value>/favicon.ico</value>
		</property>
		<property mode="concat">
			<name>pageTitle</name>
			<value> - XPages &amp; Beer CMS</value>
		</property>
		<property>
			<name>ignoreRequestParams</name>
			<value>false</value>
		</property>
		<property>
			<name>createForm</name>
			<value>true</value>
		</property>
